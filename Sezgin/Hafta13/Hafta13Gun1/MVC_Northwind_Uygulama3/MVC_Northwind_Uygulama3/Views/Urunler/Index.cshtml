@{
    // Layout = null;
}

@using MVC_Northwind_Uygulama3.Models;

@model List<Product>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table border="1">
        <thead>
            <tr>
                <th>Ürün Adı</th>
                <th>Fiyat</th>
                <th>Stok</th>
                <th>Ketegori ID</th>
                <th>Tedarikçi ID</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Product prd in Model)
            {
            <tr>
                <td>@prd.ProductName</td>
                <td>@prd.UnitPrice</td>
                <td>@prd.UnitsInStock</td>
                <td>@prd.CategoryId</td>
                <td>@prd.SupplierId</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>
